{
  "expo": {
    "name": "Secret Mission",
    "slug": "secret-mission",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/secret-mission-logo-appstore.png",
    "scheme": "secretmission",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Il gioco di societ√† per feste - Completa missioni segrete senza farti scoprire!",
    "privacy": "public",
    "platforms": ["ios"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.devcat.secretmission",
      "buildNumber": "1",
      "infoPlist": {
        "CFBundleDisplayName": "Secret Mission",
        "NSCameraUsageDescription": "This app does not use the camera.",
        "NSMicrophoneUsageDescription": "This app does not use the microphone.",
        "NSLocationWhenInUseUsageDescription": "This app does not use location services."
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/secret-mission-logo-appstore.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/secret-mission-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "your-project-id-will-be-generated"
      }
    }
  }
}
